pipeline {
    agent { label 'JAVA' }
    stages { 
        stage ("SCM Checkin-1") {
            steps {
              git branch: 'master', url: 'https://github.com/ramakanthj/SampleMaven'
            }
        }
        stage ("maven build") {
            tools {
                maven 'maven-3.6.2'
                jdk 'JDK1.8'
            }
            steps {
                sh "mvn package"
            }
     input {
                 message 'Mvn Package is Successfull, I can Proceed'
            }
        }
	}
